{
  "quilt_loader": {
    "entrypoints": {
      "init": [
        "com.oroarmor.netherite_plus.NetheritePlusMod"
      ],
      "client_init": [
        "com.oroarmor.netherite_plus.client.NetheritePlusClientMod"
      ]
    },
    "id": "netherite_plus",
    "group": "com.oroarmor",
    "version": "${version}",
    "metadata": {
      "name": "Netherite Plus Mod",
      "description": "This mod adds extra netherite features that aren't vanilla.\n\nNOTE: Editing the config requires a game restart.",
      "contributors": {
        "OroArmor": "Owner",
        "1198159": "Contributor",
        "Ron": "Texture Artist"
      },
      "contact": {
        "sources": "https://github.com/OroArmor/Netherite-Plus-Mod/",
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/netherite-plus-mod",
        "issues": "https://github.com/OroArmor/Netherite-Plus-Mod/issues"
      },
      "license": "MIT",
      "icon": "assets/netherite_plus/icon.png"
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.20.2"
      },
      {
        "id": "quilted_fabric_api",
        "versions": ">=7.2.1+0.88.0-1.20.1"
      },
      {
        "id": "minecraft",
        "versions": "1.20.1"
      },
      {
        "id": "multi_item_lib",
        "versions": ">=1.6.2"
      }
    ]
  },
  "mixin": [
    "netherite_plus.mixins.json"
  ],
  "access_widener": "netherite_plus.accesswidener",
  "minecraft": {
    "environment": "*"
  },
  "schema_version": 1
}
